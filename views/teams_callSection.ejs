<%- include("partials/leftoptions"); %>


<h3>Old Calls</h3>
<% (calls).forEach((call)=>{ %>
    <% if(call.count==0){ %>
        <a href="#">Join call with id <%= call.callID %></a>
    <% } %>
<% }) %>

<h3>Incoming Calls</h3>
<% (calls).forEach((call)=>{ %>
    <% if(call.count>0){ %>
        <a href="/team/<%= call.team_name %>/<%= call.callID %>">Join call with id <%= call.callID %></a>
    <% } %>
<% }) %>
        
